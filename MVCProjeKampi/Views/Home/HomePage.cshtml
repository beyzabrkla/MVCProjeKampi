    <script>
        // Sayfanızın en altında veya bir script dosyasında olmalı
        $(document).ready(function () {
            $("#contactForm").submit(function (e) {
                e.preventDefault(); // Formun normal submit işlemini engelle

                // Hata mesajı alanını temizle
                $("#contactMessage").empty();

                var formData = $(this).serialize(); // Formdaki tüm verileri al

                $.ajax({
                    type: "POST",
                    url: "/Contact/AddContact", // Bu, Controller/Action yolunuz olmalı
                    data: formData,
                    dataType: "json", // Sunucudan JSON bekliyoruz
                    success: function (response) {
                        if (response.success) {
                            // Başarılı mesajı göster
                            $("#contactMessage").html('<div class="alert alert-success">' + response.message + '</div>');
                            // Formu temizle
                            $("#contactForm")[0].reset();
                        } else {
                            // Sunucu Validasyon Hatası (HTTP 200, ama success: false)
                            var errorMessage = response.message + (response.detail ? ' Detay: ' + response.detail : '');
                            $("#contactMessage").html('<div class="alert alert-warning">' + errorMessage + '</div>');
                        }
                    },
                    error: function (xhr, status, error) {
                        // HTTP 500 gibi beklenmedik sunucu hatası (Bu, sizin gördüğünüz hatadır)
                        var detailedError = xhr.responseText.substring(0, 200) || error; // İlk 200 karakteri al
                        $("#contactMessage").html('<div class="alert alert-danger"><strong>Sunucudan beklenmedik bir cevap alındı.</strong> Lütfen ağ bağlantınızı kontrol edin veya yöneticinize başvurun. <br/> Hata Detayı: ' + detailedError + '</div>');
                        console.log("AJAX Error:", status, error, xhr.responseText);
                    }
                });
            });
        });
    </script>
