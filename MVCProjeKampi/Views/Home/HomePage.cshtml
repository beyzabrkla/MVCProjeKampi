    <script>
    $(document).ready(function () {
        // Form gönderildiğinde olayı yakala
        $('#contactForm').submit(function (e) {
            e.preventDefault(); // Sayfanın normal submit (yenilenme) işlemini durdur

            var formData = $(this).serialize(); // Formdaki tüm verileri al

            // AJAX çağrısı
            $.ajax({
                type: 'POST',
                url: '@Url.Action("AddContact", "Contact")', // Controller metodunun adresi
                data: formData,
                dataType: 'json', // Cevabın JSON formatında beklendiği
                success: function (response) {
                    var messageDiv = $('#contactMessage');
                    messageDiv.empty(); // Önceki mesajları temizle

                    if (response.success) {
                        // Başarılı mesajı gösterme
                        messageDiv.html('<div class="alert alert-success alert-dismissible fade show" role="alert">' +
                                        '<strong>Başarılı!</strong> ' + response.message +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');

                        // Formu temizle
                        $('#contactForm')[0].reset();
                    } else {
                        // Hata mesajı gösterme
                        messageDiv.html('<div class="alert alert-danger alert-dismissible fade show" role="alert">' +
                                        '<strong>Hata!</strong> ' + response.message +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');
                    }

                    // Kullanıcıyı mesajın olduğu yere kaydır
                    $('html, body').animate({
                        scrollTop: $("#contact").offset().top - 50
                    }, 500);
                },
                error: function (xhr, status, error) {
                    // Sunucu tarafında beklenmedik bir hata olursa
                    $('#contactMessage').html('<div class="alert alert-danger">Sunucudan beklenmedik bir cevap alındı. Lütfen ağ bağlantınızı kontrol edin.</div>');
                }
            });
        });
    });
    </script>
